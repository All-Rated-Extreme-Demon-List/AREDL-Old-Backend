    "deleteRule": null,
    "options": {
      "query": "WITH gen AS (\n  SELECT 1 AS pos\n  UNION ALL\n  SELECT pos+1 FROM gen WHERE pos+1 <= 1000\n)\nSELECT pos as id, \n  (\n  SELECT max(T.V) \n  FROM (SELECT \n  --Formular for point curve\n  ROUND(400 / SQRT((CAST(pos as FLOAT) - 1) / 50 + 0.444444) - 100, 1) \n  --\n  AS V UNION SELECT 0 AS V) T\n  )\n  as points FROM gen"
    }
